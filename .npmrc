# Skip optional dependencies to avoid platform-specific binary issues
optional=false
omit=optional

# Use npm's legacy peer deps resolution
legacy-peer-deps=true

# Disable platform-specific binaries
target_platform=linux
target_arch=x64

# Force npm to ignore optional dependencies completely
fund=false
audit=false

# Disable platform-specific Rollup binaries
rollup_platform=linux
rollup_arch=x64 