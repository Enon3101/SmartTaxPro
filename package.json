{
  "name": "myeca-admin-monorepo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "format": "prettier --write .",
    "db:generate": "cd apps/api && prisma generate",
    "db:push": "cd apps/api && prisma db push",
    "db:migrate": "cd apps/api && prisma migrate dev",
    "db:seed": "cd apps/api && tsx prisma/seed.ts",
    "seed": "npm run db:seed"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "prettier": "^3.3.3",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
